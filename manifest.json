{
    "manifest_version": 3,
   
    "name": "POMOTODO",
    "description": "Pomodoro Timer + To Do List + Not To Do List",
    "version": "1.0",
   
    "browser_action": {
      "default_icon": "/public/POMOTODO icon/icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ]
    
    "background": {
        "service_worker": "background.js"
      },
      "permissions": ["storage"]
      
  }